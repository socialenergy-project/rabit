<%= content_for :title, 'Consumer Category' %>

<div class="row">
  <div class="col-lg-12">
    <div class="card mb-3">
      <div class="card-header">
        <i class="fa fa-area-chart"></i>
        consumer_category properties
      </div>
      <div class="card-body">
                    <p>
              <strong>Name:</strong>
              
                  <%= @consumer_category.name-%>
              

            </p>

                    <p>
              <strong>Description:</strong>
              
                  <%= @consumer_category.description-%>
              

            </p>

                    <p>
              <strong>Reference year:</strong>
              
                  <%= @consumer_category.reference_year-%>
              

            </p>

        
        <%= link_to 'Edit', edit_consumer_category_path(@consumer_category), class: 'btn btn-warning btn-sm' if can? :edit, @consumer_category %> |
        <%= link_to 'All Consumer Categories', consumer_categories_path, class: 'btn btn-info btn-sm' %>

      </div>
      <div class="card-footer small text-muted">
      </div>
    </div>
  </div>
</div>

